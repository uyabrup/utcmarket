msgstr "<a href=\"!url\">Настроить блок корзины</a>."

msgid "General checkout settings"
msgstr "Общие настройки"

msgid "Enable checkout (disable to only use third party checkout service like PayPal Express Checkout)."
msgstr "Включить возможность расчёта (выключить только при использовании сервисов подобно \"PayPal Express Checkout\")."

msgid "Ubercart checkout is enabled."
msgstr "Расчёт - включено."

msgid "Ubercart checkout is disabled."
msgstr "Расчёт - выключено"

msgid "Enable anonymous checkout (users can checkout without logging in)."
msgstr "Разрешить расчёт для анонимным клиентов."

